from Look4Food.backend_atualizado.model.restaurante.restaurante import restauranteModel


class RestauranteFactory:
    @staticmethod
    def criar_restaurante(nome, cnpj, email, senha, telefone, endereco, cidade, tipo_restaurante, descricao):
        return restauranteModel(nome, cnpj, email, senha, telefone, endereco, cidade, tipo_restaurante, descricao)
